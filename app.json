{
  "expo": {
    "name": "UteBooking",
    "slug": "utelocker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesNonExemptEncryption": false,
      "bundleIdentifier": "com.utelocker.utelocker",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      }
    },
    "android": {      
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.utelocker.utelocker",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDkp0D1ojJH-LUGvGX0PhEgKanamQ-iJZw"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ba0baa70-867b-4f41-80a9-781d696d2db6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bd5b5595-a831-42eb-b03d-be929ea9f594"
    },
    "owner": "utelocker"
  }
}
